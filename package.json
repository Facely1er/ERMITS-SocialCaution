{
  "name": "project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:production": "NODE_ENV=production npm run lint && npm run type-check && npm run build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"src/**/*.css\"",
    "type-check": "tsc --noEmit",
    "analyze": "vite build --mode analyze",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "validate:production": "tsc --noEmit && npm run lint && npm run build",
    "health-check": "node scripts/health-check.cjs",
    "production-check": "node scripts/validate-production.cjs",
    "pre-deploy": "npm run production-check"
  },
  "dependencies": {
    "@huggingface/inference": "^2.6.4",
    "@sentry/react": "^10.3.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.74.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "recharts": "^2.10.3",
    "web-vitals": "^3.5.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.8",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^5.11.0",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.9",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4",
    "workbox-window": "^7.0.0"
  }
}
